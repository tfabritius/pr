{
  "coverageDirectory": "coverage/e2e",
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!**/*.spec.ts"
  ],
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
