{
  "coverageDirectory": "coverage/e2e",
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!**/*.spec.ts"
  ],
  "globals": {
    "ts-jest": {
      "astTransformers": {
        "before": ["<rootDir>/test/graphql-plugin.ts"]
      }
    }
  },
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
